#include <stdio.h>
#include <string.h>

int main(void)
{
    char string[16];
    char password[] = "please";

    puts("Please type your password");
    gets(string);

    if(strcmp(password, string) == 0)
    {
        printf("Access granted!");
    }
    else
    {
        printf("Wrong password...");
    }

}
