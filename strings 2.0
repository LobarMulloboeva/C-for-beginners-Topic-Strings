#include <stdio.h>
#include <string.h>

int main(void)
{
    char command[64];
    char response[] = "You think I know how to ";
    char new[120];

    while(strcasecmp(command, "quit"))
    {
        puts("C:\\>");
        gets(command);

        strcpy(new, response);
        strcat(new, command);
        strcat(new, "?");
        puts(new);
    }

    puts("Well, maybe I do...");
    return 0;
}
